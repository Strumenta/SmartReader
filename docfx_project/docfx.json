{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "src/SmartReader/**.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../../SmartReader.Documentation",
	 "globalMetadata": {
      "_appTitle": "SmartReader website",
      "_appFooter": "<span>Copyright Â© Strumenta.<br>Generated by <strong>DocFX</strong></span>",
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
     "template": [
      "default",
      "templates/smartreader"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}